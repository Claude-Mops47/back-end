{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "background.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "removeExpiredRefreshTokens": {
      "runtime": "@vercel/node"
    }
  },
  "events": {
    "schedule": {
      "rate": "1d",
      "enabled": true,
      "function": "removeExpiredRefreshTokens"
    }
  }
}
